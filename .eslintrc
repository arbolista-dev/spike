{
  "extends": "airbnb",
  "settings":{
	  "import/resolver":{
	  	"node":{
		  	"moduleDirectory":[
		  		"node_modules",
		  		"src"
		  	]
	  	}
	  }
  },
  "rules":{
  	"react/forbid-prop-types": 0,
  	"import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.test.js", "**/*.spec.js"]}]
  }
}
